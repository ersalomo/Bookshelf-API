{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console":"off",
        "linebreak-style":"off",
        "max-len":"off",
        "no-unused-vars":"off",
        "arrow-body-style":"off",
        "no-trailing-spaces":"off",
        "semi":"off"
    }
}
